{
   "version": 2,
   "builds": [
       {
           "source": "src/main.ts",
           "use": "@vercel/node"
       }
   ],
   "routes": [
       {
           "source": "/(.*)",
           "destination": "src/main.ts",
           "methods": [
               "GET",
               "POST",
               "PUT",
               "DELETE"
           ]
       }
   ]
}